		"devdoc": {
			"details": "Stakers contract defines data structure and methods for validators / validators.",
			"methods": {
				"contractCommission()": {
					"details": "The commission fee in percentage a validator will get from a contract, e.g., 30%"
				},
				"isOwner()": {
					"details": "Returns true if the caller is the current owner."
				},
				"maxDelegatedRatio()": {
					"details": "Maximum ratio of delegations a validator can have, say, 15 times of self-stake"
				},
				"maxLockupDuration()": {
					"details": "The maximum duration of a stake/delegation lockup, e.g. 1 year"
				},
				"minLockupDuration()": {
					"details": "The minimum duration of a stake/delegation lockup, e.g. 2 weeks"
				},
				"minSelfStake()": {
					"details": "Minimum amount of stake for a validator, i.e., 1000000 SKH"
				},
				"owner()": {
					"details": "Returns the address of the current owner."
				},
				"renounceOwnership()": {
					"details": "Leaves the contract without owner. It will not be possible to call `onlyOwner` functions anymore. Can only be called by the current owner.     * > Note: Renouncing ownership will leave the contract without an owner, thereby removing any functionality that is only available to the owner."
				},
				"transferOwnership(address)": {
					"details": "Transfers ownership of the contract to a new account (`newOwner`). Can only be called by the current owner."
				},
				"unlockedRewardRatio()": {
					"details": "The ratio of the reward rate at base rate (no lock), e.g., 30%"
				},
				"validatorCommission()": {
					"details": "The commission fee in percentage a validator will get from a delegation, e.g., 15%"
				},
				"version()": {
					"details": "Returns the address of the current owner."
				},
				"withdrawalPeriodEpochs()": {
					"details": "the number of epochs that stake is locked"
				}
			}
		},
